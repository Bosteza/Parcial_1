Este programa suma N enteros
#include<stdio.h>

int main()
{

  
    int num_inicial, num_final; 
    
    printf("Pon un n√∫mero\n");
    scanf("%d", &num_inicial);

    num_final=(num_inicial*(num_inicial+1))/2;
    

    
    printf("%d", num_final);
    printf("\n");
    
     
        
    
    

    return 0;
}

