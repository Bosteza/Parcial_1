Este programa imprime impares 

int main()
{

   
    int num_inicial, num_final; 
    
    printf("Pon un n√∫mero\n");
    scanf("%d", &num_inicial);

   
    num_final = num_inicial + 20;

    
    while (num_inicial <= num_final)
    {

        if (num_inicial %2 != 0){
            printf("%d", num_inicial);
            printf("\n");
        }
        num_inicial++;
    }
    

    return 0;
}
